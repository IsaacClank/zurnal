{
  "compilerOptions": {
    "incremental": true,     
    "target": "es2020",     
    "module": "commonjs",  
    "lib": ["ESNext"],
    "strict": true,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    // "noFallthroughCasesInSwitch": true,
    // "noUncheckedIndexedAccess": true, 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "dist",
  }
}
